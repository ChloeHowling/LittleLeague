<div id="teams" class="container my-5"><br>
  <main class="">
    <h1 class="text-white">
      <%=viewModel.listTitle%>
    </h1>
    <div class="row">
      <div class="col-12 d-flex justify-content-start">
        <input id="searchInput" class=" me-2 p-1" placeholder="Search...">
        <button id="clearSearch" class="btn btn-secondary me-2">Clear</button>
        <!-- <button id="resetView" class="btn btn-primary clear-search-btn ml-2">Reset View</button> -->
      </div>
    </div>
    <div class="col-12">
      <div id="<%=viewModel.alertContainerId%>" class="mt-2"></div>
      <div id="<%=viewModel.containerId%>" class="mt-2"></div>
      <button id="newButton" class="btn btn-primary" data-bs-toggle="modal"
        data-bs-target="#<%=viewModel.editModalContainerId%>">Add New</button>
    </div>
</div>
</main>

<div class="modal" id="deleteConfirmation" tabindex="-1" aria-labelledby="deleteConfirmationLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <!-- Modal Header -->
      <div class="modal-header">
        <h5 class="modal-title">Confirmation</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <!-- Modal body -->
      <div class="modal-body">
        <p>Are you sure you want to delete this team and all its information?</p>
      </div>
      <!-- Modal footer -->
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
        <button id="yesButton" type="button" class="btn btn-danger" data-bs-dismiss="modal">delate</button>
      </div>
    </div>
  </div>
</div>

<div class="modal" id="editModal" tabindex="-1" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div id="formContainer">
      </div>
    </div>
  </div>
</div>
</div>